{
  "extends": [
    "eslint:recommended",
    "standard"
  ],
  "rules": {
    "no-console": 0,
    "max-len": [
      "error",
      {
        "tabWidth": 2,
        "ignoreUrls": true,
        "ignoreStrings": true,
        "ignoreTemplateLiterals": true,
        "ignoreRegExpLiterals": true
      }
    ]
  }
}
